<div class="row">
  <div class="col-md-4">
    <h3>Travel Method</h3>
    <%= render 'application/entity_control_links', entity: @travel_method %>
    <%= render 'application/entity_timestamps', entity: @travel_method %>

    <table class="table">
      <tr>
        <td nowrap>
          <strong>Name:</strong>
        </td>
        <td nowrap>
          <%= @travel_method.name %>
        </td>
      </tr>
    </table>

  </div>
</div>
