<% unless folder_url.nil? %>
    <a class="btn btn-default" href="<%= folder_url %>" target="_blank" title="Go to folder in Alfresco">
      <span class="glyphicon glyphicon glyphicon-log-in" aria-hidden="true"></span>
    </a>
<% else %>
    <a class="btn btn-default" href="#" title="Unable to get folder link" disabled>
      <span class="glyphicon glyphicon glyphicon glyphicon-remove-circle" aria-hidden="true"></span>
    </a>
<% end %>